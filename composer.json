{
    "name": "andrewmontagne/moduleloader",
    "description": "A library for module discovery within a project.",
    "type": "library",
    "bin": [
        "src/main/bin/generate-module-manifest"
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "MuPHP\\": "src/main/php/MuPHP"
        },
        "files": [
            "src/main/php/Inject.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MuPHP\\": "src/test/php/MuPHP"
        }
    },
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Andrew O'Rourke",
            "email": "andrew@montagne.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "ext-json": "*"
    }
}
